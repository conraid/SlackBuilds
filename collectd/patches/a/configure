$as_echo "$c_cv_have_netsnmp_old_api" >&6; }

  if test "x$c_cv_have_netsnmp_old_api" = "xyes"; then

$as_echo "#define HAVE_NETSNMP_OLD_API 1" >>confdefs.h

  fi

