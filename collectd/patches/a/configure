{
return rte_eal_init(0, NULL);

  ;
  return 0;
}

_ACEOF
if ac_fn_c_try_link "$LINENO"; then :
  with_libdpdk="yes"
else
  with_libdpdk="no (symbol 'rte_eal_init' not found)"

fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
  LIBS="$SAVE_LIBS"
