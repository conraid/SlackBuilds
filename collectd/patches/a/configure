    pkg_failed=untried
fi
if test -n "$LIBMONGOC_LIBS"; then
    pkg_cv_LIBMONGOC_LIBS="$LIBMONGOC_LIBS"
 elif test -n "$PKG_CONFIG"; then
    if test -n "$PKG_CONFIG" && \
    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"libmongoc-1.0\""; } >&5
