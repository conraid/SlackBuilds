  SAVE_LIBS="$LIBS"
    PERL_CFLAGS=`ARCHFLAGS="" $PERL -MExtUtils::Embed -e perl_inc`
  PERL_LIBS=`ARCHFLAGS="" $PERL -MExtUtils::Embed -e ldopts`
  CFLAGS="$CFLAGS $PERL_CFLAGS"
  LIBS="$LIBS $PERL_LIBS"

  { $as_echo "$as_me:${as_lineno-$LINENO}: checking for libperl" >&5
$as_echo_n "checking for libperl... " >&6; }
if ${c_cv_have_libperl+:} false; then :
  $as_echo_n "(cached) " >&6
else

      cat confdefs.h - <<_ACEOF >conftest.$ac_ext
