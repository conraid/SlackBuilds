diff -Naupr bluefish-2.2.8/src/plugin_rsync/rsync.c bluefish/src/plugin_rsync/rsync.c
--- bluefish-2.2.8/src/plugin_rsync/rsync.c	1970-01-01 01:00:00.000000000 +0100
+++ bluefish/src/plugin_rsync/rsync.c	2010-08-28 19:01:54.000000000 +0200
@@ -0,0 +1,126 @@
+/* Bluefish HTML Editor
+ * rsync.c - plugin for rsync
+ *
+ * Copyright (C) 2005 Olivier Sessink
+ *
